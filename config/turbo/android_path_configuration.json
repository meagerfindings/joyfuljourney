{
  "settings": {
    "pull_to_refresh_enabled": true,
    "show_bars_on_scroll": true,
    "page_refresh_delay": 500,
    "screenshot_enabled": true
  },
  "rules": [
    {
      "patterns": [
        "/login",
        "/users/new"
      ],
      "properties": {
        "presentation": "modal",
        "pull_to_refresh_enabled": false,
        "title": "Account",
        "toolbar_enabled": false
      }
    },
    {
      "patterns": [
        "/posts/new",
        "/posts/.*/edit"
      ],
      "properties": {
        "presentation": "modal",
        "pull_to_refresh_enabled": false,
        "title": "Post",
        "toolbar_enabled": true
      }
    },
    {
      "patterns": [
        "/users/.*/edit"
      ],
      "properties": {
        "presentation": "modal",
        "pull_to_refresh_enabled": false,
        "title": "Edit Profile",
        "toolbar_enabled": true
      }
    },
    {
      "patterns": [
        "/families/new",
        "/families/.*/edit"
      ],
      "properties": {
        "presentation": "modal",
        "pull_to_refresh_enabled": false,
        "title": "Family",
        "toolbar_enabled": true
      }
    },
    {
      "patterns": [
        "/milestones/new",
        "/milestones/.*/edit"
      ],
      "properties": {
        "presentation": "modal",
        "pull_to_refresh_enabled": false,
        "title": "Milestone",
        "toolbar_enabled": true
      }
    },
    {
      "patterns": [
        "/relationships/new"
      ],
      "properties": {
        "presentation": "modal",
        "pull_to_refresh_enabled": false,
        "title": "Add Relationship",
        "toolbar_enabled": true
      }
    },
    {
      "patterns": [
        "/"
      ],
      "properties": {
        "presentation": "replace_root",
        "pull_to_refresh_enabled": true,
        "bottom_navigation_item": {
          "title": "Home",
          "icon": "ic_home",
          "index": 0
        }
      }
    },
    {
      "patterns": [
        "/posts",
        "/posts/\\d+"
      ],
      "properties": {
        "presentation": "push",
        "pull_to_refresh_enabled": true,
        "bottom_navigation_item": {
          "title": "Posts",
          "icon": "ic_edit",
          "index": 1
        }
      }
    },
    {
      "patterns": [
        "/timeline",
        "/timeline/truncated"
      ],
      "properties": {
        "presentation": "push",
        "pull_to_refresh_enabled": true,
        "bottom_navigation_item": {
          "title": "Timeline",
          "icon": "ic_schedule",
          "index": 2
        }
      }
    },
    {
      "patterns": [
        "/families",
        "/families/\\d+"
      ],
      "properties": {
        "presentation": "push",
        "pull_to_refresh_enabled": true,
        "bottom_navigation_item": {
          "title": "Family",
          "icon": "ic_group",
          "index": 3
        }
      }
    },
    {
      "patterns": [
        "/users/\\d+"
      ],
      "properties": {
        "presentation": "push",
        "pull_to_refresh_enabled": false,
        "bottom_navigation_item": {
          "title": "Profile",
          "icon": "ic_person",
          "index": 4
        }
      }
    },
    {
      "patterns": [
        "/users",
        "/relationships"
      ],
      "properties": {
        "presentation": "push",
        "pull_to_refresh_enabled": true
      }
    },
    {
      "patterns": [
        "/milestones",
        "/milestones/\\d+"
      ],
      "properties": {
        "presentation": "push",
        "pull_to_refresh_enabled": true,
        "title": "Milestones"
      }
    },
    {
      "patterns": [
        ".*"
      ],
      "properties": {
        "presentation": "push",
        "pull_to_refresh_enabled": true
      }
    }
  ]
}