<h1>All Users</h1>

<%= link_to 'Create User', '/users/new' %>

<ul>
  <% @users.each do |user| %>
    <li id="user-<%= user.id %>">
      <%= link_to user.name, user_path(user.id) %> - <%= user.birthdate %> - <%= user.created_at %> -  <%= link_to "Edit", edit_user_path(user.id) %>
    </li>
  <% end %>
</ul>
